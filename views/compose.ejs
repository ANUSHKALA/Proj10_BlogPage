<%- include('partials/header') -%>

<h1>Compose</h1>
<form action="/compose" method="post">
    <label>Title</label>
    <input style="background-color: rgb(197, 223, 217);" class="form-control" type="text" name="blogTitle">
    <br>
    <br>
    <label>Post</label>
    <textarea style="background-color: rgb(197, 223, 217);" class="form-control" rows="10" name="blogBody"></textarea>
    <br>
    <button class="btn btn-primary" type="submit">Publish</button>
</form>

<%- include('partials/footer') -%>